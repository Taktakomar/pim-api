<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Farbe" table="farbe">
    <id name="farbeId" type="integer" column="farbe_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="farbeName" type="string" column="farbe_name" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="farbeBeschreibung" type="string" column="farbe_beschreibung" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
