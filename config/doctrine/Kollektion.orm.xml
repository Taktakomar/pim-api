<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Kollektion" table="kollektion">
    <id name="kollektionId" type="integer" column="kollektion_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kollektionName" type="string" column="kollektion_name" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kollektionBeschreibung" type="string" column="kollektion_beschreibung" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
